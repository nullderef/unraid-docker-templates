<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2017-11-10</Date>
  <Name>backup-node</Name>
  <Description></Description>
  <Project>https://censoredgames.co.uk/</Project>
  <Registry>https://hub.docker.com/r/censoredgames/backup-node/</Registry>
  <Repository>censoredgames/backup-node</Repository>
  <GitHub>https://github.com/nullderef/backup-node</GitHub>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name>USERNAME</Name>
      <Value>backups</Value>
    </Variable>
    <Variable>
      <Name>PASSWORD</Name>
      <Value>changeme</Value>
    </Variable>
    <Variable>
      <Name>NFSSERVER</Name>
      <Value>localhost</Value>
    </Variable>
    <Variable>
      <Name>NFSMOUNT</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8070</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
     </Publish>
  </Networking>
  <Icon>http://icons.iconarchive.com/icons/hopstarter/soft-scraps/256/Save-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
