<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2018-01-23</Date>
  <Name>nextcloud</Name>
  <Description></Description>
  <Project>https://nextcloud.com/</Project>
  <Registry>https://hub.docker.com/r/censoredgames/nextcloud/</Registry>
  <Repository>censoredgames/nextcloud</Repository>
  <GitHub>https://github.com/nullderef/nextcloud</GitHub>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name>HTTP_WEB_PORT</Name>
      <Value>80</Value>
    </Variable>
    <Variable>
      <Name>HTTP_ALIAS</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9060</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
     </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/nextcloud/config</HostDir>
      <ContainerDir>/nextcloud/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/nextcloud/logs</HostDir>
      <ContainerDir>/nextcloud/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/nextcloud/data</HostDir>
      <ContainerDir>/nextcloud/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
<WebUI>http://[IP]:[PORT:80]</WebUI>
  <Icon>https://github.com/nullderef/nextcloud/raw/master/icon/nextcloud.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
