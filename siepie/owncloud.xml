<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>False</Beta>
  <Category></Category>
  <Date>2017-11-10</Date>
  <Name>owncloud</Name>
  <Description></Description>
  <Project>https://owncloud.com/</Project>
  <Registry>https://hub.docker.com/r/siepie/owncloud/</Registry>
  <Repository>siepie/owncloud</Repository>
  <GitHub>https://github.com/nullderef/owncloud</GitHub>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment>
    <Variable>
      <Name>HTTP_WEB_PORT</Name>
      <Value>80</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9060</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
     </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/owncloud/config</HostDir>
      <ContainerDir>/owncloud/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/owncloud/logs</HostDir>
      <ContainerDir>/owncloud/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/owncloud/data</HostDir>
      <ContainerDir>/owncloud/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
<WebUI>http://[IP]:[PORT:80]</WebUI>
  <Icon>https://owncloud.org/wp-content/themes/owncloudorgnew/assets/img/common/owncloud-square-logo.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
